    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "express-async-errors": "^2.1.1",
    "express-sslify": "^1.2.0",
    "helmet": "^3.11.0",
    "html-to-mrkdwn": "^2.0.0",
    "jsonwebtoken": "^8.1.0",
    "keyv": "^3.0.0",
    "moment": "^2.21.0",
    "named-regexp": "^0.1.1",
    "newrelic": "^3.0.0",
    "pg": "^7.4.1",
    "probot": "^6.0.0",
    "query-string": "^5.0.1",
    "raven": "^2.4.1",
    "request": "^2.83.0",
