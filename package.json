{
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "probot run --webhook-path=/github/events ./lib",
    "test": "jest --coverage --runInBand --forceExit",
    "posttest": "npm run lint",
    "lint": "eslint lib test",
    "dev": "nodemon --exec npm start",
    "migrate": "sequelize db:migrate"
  },
  "dependencies": {
    "@keyv/redis": "^1.3.8",
    "@slack/client": "^3.16.0",
    "axios": "^0.17.1",
    "body-parser": "^1.17.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "github": "^13.0.1",
    "html-to-mrkdwn": "github:github-slack/html-to-mrkdwn",
    "jsonwebtoken": "^8.1.0",
    "keyv": "^3.0.0",
    "moment": "^2.18.1",
    "named-regexp": "^0.1.1",
    "pg": "^6.4.2",
    "probot": "^5.0.0",
    "query-string": "^5.0.1",
    "request": "^2.83.0",
    "sequelize": "^4.29.0",
    "sequelize-cli": "^3.1.0"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-probot": ">=0.1.0",
    "eslint-plugin-import": "^2.8.0",
    "expect": "^22.0.3",
    "jest": "^22.2.2",
    "jest-cli": "^22.2.2",
    "localtunnel": "^1.8.3",
    "nock": "^9.1.0",
    "nodemon": "^1.11.0",
    "snappydoo": "github:github-slack/snappydoo",
    "supertest": "github:bkeepers/supertest#unknown-errors"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./test/setup"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  },
  "snappydoo": {
    "in": "test/slack/renderer",
    "out": "test/slack/renderer/renders",
    "exclude": [
      "AbstractIssue",
      "Message"
    ]
  }
}
