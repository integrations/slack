    "jest": "^22.2.2",
    "jest-cli": "^22.2.2",
    "localtunnel": "^1.8.3",
    "nock": "^9.1.0",
    "nodemon": "^1.11.0",
    "primer-core": "^6.7.0",
    "primer-marketing": "^6.2.0",
    "snappydoo": "github:github-slack/snappydoo",
    "supertest": "github:bkeepers/supertest#unknown-errors"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./test/setup"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  },
  "snappydoo": {
    "in": "test/messages",
    "out": "test/messages/renders",
    "exclude": [
      "AbstractIssue",
      "Message"
    ]
  }
